package CompareRates;

import java.io.File;

import org.junit.Before;
import org.junit.Test;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;




public class Main {

	public WebDriver DRIVER;
	@Before
	public void init() {
		
	}
	
	@Test
	public void setChromeBrowser() {
		File file = new File("../Libraries/chromedriver");
		System.setProperty("webdriver.chrome.driver", file.getAbsolutePath());
		DRIVER = new ChromeDriver();
		
		DRIVER.get("http://www.bankrate.com");
	}
	
	public static void main(String args[]) {
		try
		{
			SQL_Queries qry1 = new SQL_Queries();
			qry1.insertInto_BankRate("ZakirTest", 55);
			qry1.insertInto_BankRate("CD", 5.5f);
			qry1.sql_closeConn();
			
		}
		catch (Exception e)
		{
		  System.err.println("Got an exception! ");
		  System.err.println(e.getMessage());
		}
		
		System.out.println("END");
	}
}
