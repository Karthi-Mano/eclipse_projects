package CompareRates;

import java.sql.Connection;
import java.sql.DatabaseMetaData;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.Statement;



public class MainCompareRates {

	public static void main(String args[]) {
		try
		{
		  // create a java mysql database connection
		  String myDriver = "org.gjt.mm.mysql.Driver";
		  String myUrl = "jdbc:mysql://localhost/WebDriverProjects";
		  Class.forName(myDriver);
		  Connection conn = DriverManager.getConnection(myUrl, "root", "");

		  DatabaseMetaData dbMeta = conn.getMetaData();
		  //int col_count = dbMeta.getColumn (null, null, "Bank%",  null);
		  ResultSet rs = dbMeta.getTables(null, null, "Bank%", null);

		  String sqlStatement = "INSERT INTO `BankRate` (`LineOfBusiness`, `RatePercent`) " + 
				  				"VALUES ('Credit Cards', 88.23)";
		  
		  Statement stmt = conn.createStatement();
		  stmt.execute(sqlStatement);
		  conn.close();
		}
		catch (Exception e)
		{
		  System.err.println("Got an exception! ");
		  System.err.println(e.getMessage());
		}
		
		System.out.println("END");
	}
}
